{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/ai-ppt-generator.html"
    },
    {
      "source": "/api-diagnosis",
      "destination": "/api-diagnosis.html"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": false
    }
  ]
}
